{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "lint:eslint": "eslint --ext \".ts,.vue\" --ignore-path .gitignore .",
    "lint:eslint:fix": "eslint --ext \".ts,.vue\" --ignore-path .gitignore . --fix",
    "lint:prettier": "prettier --check .",
    "lint:prettier:fix": "prettier --write --list-different .",
    "lint": "bun lint:eslint && bun lint:prettier",
    "lint:fix": "bun lint:eslint:fix && bun lint:prettier:fix"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.5.1",
    "nuxt": "^3.11.2",
    "primevue": "^3.52.0",
    "vue": "^3.4.26",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/test-utils": "^3.12.1",
    "@pinia/testing": "^0.1.3",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "happy-dom": "^13.10.1",
    "msw": "^2.2.14",
    "nuxt-primevue": "^0.3.1",
    "playwright-core": "^1.43.1",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  }
}
